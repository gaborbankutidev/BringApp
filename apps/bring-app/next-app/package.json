{
	"name": "next-app",
	"version": "1.7.0",
	"private": true,
	"scripts": {
		"build:blocks": "wp-scripts build editor.config.ts --output-filename=../../plugins/bring-app/build/blocks.js --config webpack.editor.config.js",
		"build:next": "next build",
		"build:next:ci": "dotenv -e .env.test -- yarn build:next",
		"build:storybook": "storybook build",
		"build:tailwind": "tailwindcss -i ./src/styles/globals.css -o ../plugins/bring-app/build/tailwind.css --minify",
		"checks": "conc yarn:types yarn:lint --success=all",
		"format": "prettier --write .",
		"lint": "next lint",
		"start": "next start",
		"types": "tsc --noEmit",
		"watch:blocks": "wp-scripts start editor.config.ts --output-filename=../../plugins/bring-app/build/blocks.js --config webpack.editor.config.js",
		"watch:next": "NODE_TLS_REJECT_UNAUTHORIZED=0 next dev --turbo",
		"watch:storybook": "storybook dev -p 6006",
		"watch:tailwind": "tailwindcss -i ./src/styles/globals.css -o ../plugins/bring-app/build/tailwind.css --watch"
	},
